{
  "name": "react-liquid-gauge",
  "version": "1.2.5",
  "description": "React Liquid Gauge component",

  "scripts": {
    "prepublish": "npm run lint && npm test && npm run build",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "build-examples": "cd examples; webpack",
    "clean": "rm -f lib/*",
    "lint": "eslint lib",
    "lint:fix": "eslint --fix lib",
    "test": "tap test/*.js --node-arg=--require --node-arg=babel-register --node-arg=--require --node-arg=babel-polyfill",
    "coveralls": "tap test/*.js --coverage --coverage-report=text-lcov --nyc-arg=--require --nyc-arg=babel-register --nyc-arg=--require --nyc-arg=babel-polyfill | coveralls",
    "dev": "cd examples; webpack-dev-server --hot --inline --host 0.0.0.0 --port 8000 --content-base ../docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/The-Funk/react-liquid-gauge.git"
  },
  "author": "Cheton Wu <cheton@gmail.com>",
  "contributors": [
    {
      "name": "Cheton Wu",
      "email": "cheton@gmail.com",
      "url": "https://github.com/cheton"
    },
    {
      "name": "Cole Snyder",
      "email": "icsnyder42@gmail.com",
      "url": "https://github.com/The-Funk"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/trendmicro-frontend/react-liquid-gauge/issues"
  },
  "homepage": "https://github.com/trendmicro-frontend/react-liquid-gauge",
  "keywords": [
    "react",
    "react-component",
    "d3",
    "liquid",
    "gauge",
    "chart"
  ],
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "dependencies": {
    "d3-color": "^2.0.0",
    "d3-ease": "^2.0.0",
    "d3-interpolate": "^2.0.1",
    "d3-scale": "^3.3.0",
    "d3-selection": "^2.0.0",
    "d3-shape": "^2.1.0",
    "d3-timer": "^2.0.0",
    "d3-transition": "^2.0.0",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.13.0",
    "@trendmicro/react-buttons": "~1.3.1",
    "coveralls": "~3.1.0",
    "eslint": "~7.24.0",
    "eslint-plugin-import": "~2.22.1",
    "eslint-plugin-jsx-a11y": "~6.4.1",
    "eslint-plugin-react": "~7.23.2",
    "react-scripts": "^4.0.3",
    "tap": "~15.0.4",
    "trendmicro-ui": "~0.5.2",
    "which": "~2.0.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
